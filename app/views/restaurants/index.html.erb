<h1>Restaurants#index</h1>
<p>Find me in app/views/restaurants/index.html.erb</p>
<%= link_to "Create new restaurant", new_restaurant_path %>

<% @restaurants.each do |r| %>
  <hr class="solid">
  <h4> <%= link_to r.name, restaurant_path(r) %> </h4>
  <p> <%= r.address %> </p>
  <p> <%= r.category.upcase %> </p>
<% end %>
